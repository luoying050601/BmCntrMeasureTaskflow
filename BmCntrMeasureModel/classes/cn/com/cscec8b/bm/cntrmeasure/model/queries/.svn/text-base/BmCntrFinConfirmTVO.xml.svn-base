  <SQLQuery>
    <![CDATA[SELECT cvh.cntr_id,
       cvh.project_id,
       sch.K_NUMBER                                  as cntr_number,
       BmCntrFinConfirmTEO.DOCUMENT_NUMBER,
       cvh.verify_start_date,
       BmCntrFinConfirmTEO.TRANSFER_STATUS,
       cvh.verify_end_date,
       sch.K_ALIAS                                   as cntr_name,
       cvh.document_number                           as verify_document_number,
       scv.CUSTOMER_NAME,
       BmCntrFinConfirmTEO.CONFIRM_DATE,
       scv.CUSTOMER_NUMBER,
       p.segment1                                    as project_number,
       p.NAME                                        as project_name,
       cvh.ower_confirm_quantity,
       cvh.actual_confirm_date,
       BmCntrFinConfirmTEO.Invoice_Number,
       BmCntrFinConfirmTEO.CNTR_VERIFY_HEADER_ID,
       BmCntrFinConfirmTEO.CUMULATIVE_CONFIRM_AMOUNT,
       BmCntrFinConfirmTEO.CONFIRM_AMOUNT,
       
       BmCntrFinConfirmTEO.TRANSFER_ERROR_MSG,
       BmCntrFinConfirmTEO.CREATION_DATE,
       BmCntrFinConfirmTEO.CREATED_BY,
       BmCntrFinConfirmTEO.LAST_UPDATED_BY,
       BmCntrFinConfirmTEO.LAST_UPDATE_DATE,
       BmCntrFinConfirmTEO.ATTRIBUTE_CATEGORY,
       BmCntrFinConfirmTEO.ATTRIBUTE1,
       BmCntrFinConfirmTEO.ATTRIBUTE2,
       BmCntrFinConfirmTEO.ATTRIBUTE3,
       BmCntrFinConfirmTEO.ATTRIBUTE4,
       BmCntrFinConfirmTEO.ATTRIBUTE5,
       BmCntrFinConfirmTEO.ATTRIBUTE6,
       BmCntrFinConfirmTEO.ATTRIBUTE7,
       BmCntrFinConfirmTEO.ATTRIBUTE8,
       BmCntrFinConfirmTEO.ATTRIBUTE9,
       BmCntrFinConfirmTEO.ATTRIBUTE10,
       BmCntrFinConfirmTEO.ATTRIBUTE11,
       BmCntrFinConfirmTEO.ATTRIBUTE12,
       BmCntrFinConfirmTEO.ATTRIBUTE13,
       BmCntrFinConfirmTEO.ATTRIBUTE14,
       BmCntrFinConfirmTEO.ATTRIBUTE15
  FROM BM.BM_CNTR_FIN_CONFIRM_T   BmCntrFinConfirmTEO,
       BM.BM_CNTR_VERIFY_HEADER_T cvh,
       stm.stm_projects_v         p,
       stm.stm_contract_headers_v sch,
       stm.stm_customers_v        scv
 where BmCntrFinConfirmTEO.Cntr_Verify_Header_Id =
       cvh.cntr_verify_header_id(+)
   and cvh.project_id = p.project_id(+)
   and cvh.cntr_id = sch.K_HEADER_ID(+)
   and cvh.customer_id = scv.CUSTOMER_ID(+)
and cvh.project_id = :bvProjectId]]>
  </SQLQuery>
