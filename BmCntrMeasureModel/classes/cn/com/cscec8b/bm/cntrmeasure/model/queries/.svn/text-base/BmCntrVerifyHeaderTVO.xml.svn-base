  <SQLQuery>
    <![CDATA[select t.*,
       (Ower_Confirm_Quantity - ALREADY_CONFIRM_AMOUNT) AS LEFT_CONFIRM_AMOUNT
  from (SELECT BmCntrVerifyHeaderTEO.CNTR_ID,
               sch.K_NUMBER as cntr_number,
               BmCntrVerifyHeaderTEO.VERIFY_START_DATE,
               BmCntrVerifyHeaderTEO.STATUS,
               
               BmCntrVerifyHeaderTEO.VERIFY_END_DATE,
               BmCntrVerifyHeaderTEO.DOCUMENT_NUMBER,
               sch.K_ALIAS as cntr_name,
               BmCntrVerifyHeaderTEO.CUSTOMER_ID,
               scv.CUSTOMER_NAME,
               scv.CUSTOMER_NUMBER,
               p.segment1 project_number,
               p.NAME as project_name,
               BmCntrVerifyHeaderTEO.CNTR_CONFIRM_DATE,
               BmCntrVerifyHeaderTEO.ACTUAL_CONFIRM_DATE,
               (to_date(BmCntrVerifyHeaderTEO.CNTR_CONFIRM_DATE) -
               to_date(BmCntrVerifyHeaderTEO.ACTUAL_CONFIRM_DATE)) as Owner_Measure_Delay_time,
               BmCntrVerifyHeaderTEO.CNTR_PAYMENT_DATE,
               BmCntrVerifyHeaderTEO.ACTUAL_PAYMENT_DATE,
               (to_date(BmCntrVerifyHeaderTEO.CNTR_PAYMENT_DATE) -
               to_date(BmCntrVerifyHeaderTEO.ACTUAL_PAYMENT_DATE)) as Owner_Payment_Delay_time,
               BmCntrVerifyHeaderTEO.ACTUAL_OUTPUT,
               BmCntrVerifyHeaderTEO.ACTUAL_PAYMENT_AMOUNT,
               NVL((select sum(cfc.confirm_amount)
                     from bm.bm_cntr_fin_confirm_t cfc
                    where cfc.transfer_status = 'SUCCESS'
                      AND BmCntrVerifyHeaderTEO.STATUS = 'CONFIRMED'
                      AND BmCntrVerifyHeaderTEO.Cntr_Verify_Header_Id =
                          CFC.CNTR_VERIFY_HEADER_ID
                    group by cfc.cntr_verify_header_id),
                   0) AS ALREADY_CONFIRM_AMOUNT,
               BmCntrVerifyHeaderTEO.ATTRIBUTE1,
               BmCntrVerifyHeaderTEO.ATTRIBUTE10,
               BmCntrVerifyHeaderTEO.ATTRIBUTE11,
               BmCntrVerifyHeaderTEO.ATTRIBUTE12,
               BmCntrVerifyHeaderTEO.ATTRIBUTE13,
               BmCntrVerifyHeaderTEO.ATTRIBUTE14,
               BmCntrVerifyHeaderTEO.ATTRIBUTE15,
               BmCntrVerifyHeaderTEO.ATTRIBUTE2,
               BmCntrVerifyHeaderTEO.ATTRIBUTE3,
               BmCntrVerifyHeaderTEO.ATTRIBUTE4,
               BmCntrVerifyHeaderTEO.ATTRIBUTE5,
               BmCntrVerifyHeaderTEO.ATTRIBUTE6,
               BmCntrVerifyHeaderTEO.ATTRIBUTE7,
               BmCntrVerifyHeaderTEO.ATTRIBUTE8,
               BmCntrVerifyHeaderTEO.ATTRIBUTE9,
               BmCntrVerifyHeaderTEO.ATTRIBUTE_CATEGORY,
               BmCntrVerifyHeaderTEO.CNTR_PAYMENT_AMOUNT,
               BmCntrVerifyHeaderTEO.CNTR_VERIFY_HEADER_ID,
               BmCntrVerifyHeaderTEO.CNTR_VISA_CONFIRM_QUANTITY,
               BmCntrVerifyHeaderTEO.CREATED_BY,
               BmCntrVerifyHeaderTEO.CREATION_DATE,
               BmCntrVerifyHeaderTEO.LABOR_AMOUNT,
               BmCntrVerifyHeaderTEO.LAST_UPDATE_DATE,
               BmCntrVerifyHeaderTEO.LAST_UPDATED_BY,
               BmCntrVerifyHeaderTEO.OBJECT_VERSION_NUMBER,
               BmCntrVerifyHeaderTEO.OTHER_SUBCNTR_OUTPUT,
               BmCntrVerifyHeaderTEO.OWER_CONFIRM_QUANTITY,
               BmCntrVerifyHeaderTEO.PREPARED_BY,
               sev.full_name as PREPARED_BY_name,
               BmCntrVerifyHeaderTEO.PREPARED_DATE,
               BmCntrVerifyHeaderTEO.PRO_SUBCNTR_OUTPUT,
               BmCntrVerifyHeaderTEO.PROJECT_ID,
               BmCntrVerifyHeaderTEO.SECURITY_AMOUNT,
               BmCntrVerifyHeaderTEO.CUMULATIVE_ACTUAL_OUTPUT,
               (select t.tax_rate
                  from bm.bm_cntr_list_header_t t
                 where T.CNTR_ID = BmCntrVerifyHeaderTEO.CNTR_ID
                   AND ROWNUM = 1) as tax_rate
          FROM BM.BM_CNTR_VERIFY_HEADER_T BmCntrVerifyHeaderTEO,
               stm.stm_projects_v         p,
               stm.stm_contract_headers_v sch,
               stm.stm_customers_v        scv,
               stm.stm_employees_v        sev
         where p.project_id(+) = BmCntrVerifyHeaderTEO.Project_Id
           and sch.K_HEADER_ID(+) = BmCntrVerifyHeaderTEO.Cntr_Id
           and scv.CUSTOMER_ID(+) = BmCntrVerifyHeaderTEO.Customer_Id
           and BmCntrVerifyHeaderTEO.Prepared_By = sev.employee_id(+)) t]]>
  </SQLQuery>
