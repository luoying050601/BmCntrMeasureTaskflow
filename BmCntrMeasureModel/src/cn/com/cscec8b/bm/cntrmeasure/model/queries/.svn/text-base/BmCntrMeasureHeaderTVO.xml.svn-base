  <SQLQuery>
    <![CDATA[SELECT BmCntrMeasureHeaderTEO.CNTR_MEASURE_HEADER_ID,
      
       sch.K_NUMBER                                  as cntr_number,
       BmCntrMeasureHeaderTEO.Measure_Date as MEASURE_START_time,
       BmCntrMeasureHeaderTEO.STATUS,
       sch.K_ALIAS                                   as cntr_name,
       (BmCntrMeasureHeaderTEO.Measure_Date)  as MEASURE_end_time,
       BmCntrMeasureHeaderTEO.DOCUMENT_NUMBER,
       scv.CUSTOMER_NAME,
       BmCntrMeasureHeaderTEO.PROJECT_ID,
       supa.segment1                                 as project_number,
       supa.name                                     as project_name,
       BmCntrMeasureHeaderTEO.CNTR_ID,
       BmCntrMeasureHeaderTEO.CUSTOMER_ID,
       scv.CUSTOMER_NUMBER,
       BmCntrMeasureHeaderTEO.PREPARED_BY,
       sev.full_name as prepared_by_name,
       BmCntrMeasureHeaderTEO.PREPARED_DATE,
       BmCntrMeasureHeaderTEO.CNTR_LIST_NUMBER,
       BmCntrMeasureHeaderTEO.PRODUCTION_TOTAL,
       BmCntrMeasureHeaderTEO.PRODUCTION_PRO_SUBCNTR,
       BmCntrMeasureHeaderTEO.ACCOUNT_FLAG,
       BmCntrMeasureHeaderTEO.OBJECT_VERSION_NUMBER,
       BmCntrMeasureHeaderTEO.CREATION_DATE,
       BmCntrMeasureHeaderTEO.CREATED_BY,
       BmCntrMeasureHeaderTEO.LAST_UPDATED_BY,
       BmCntrMeasureHeaderTEO.LAST_UPDATE_DATE,
       BmCntrMeasureHeaderTEO.ATTRIBUTE_CATEGORY,
       BmCntrMeasureHeaderTEO.ATTRIBUTE1,
       BmCntrMeasureHeaderTEO.ATTRIBUTE2,
       BmCntrMeasureHeaderTEO.ATTRIBUTE3,
       BmCntrMeasureHeaderTEO.ATTRIBUTE4,
       BmCntrMeasureHeaderTEO.ATTRIBUTE5,
       BmCntrMeasureHeaderTEO.ATTRIBUTE6,
       BmCntrMeasureHeaderTEO.ATTRIBUTE7,
       BmCntrMeasureHeaderTEO.ATTRIBUTE8,
       BmCntrMeasureHeaderTEO.ATTRIBUTE9,
       BmCntrMeasureHeaderTEO.ATTRIBUTE10,
       BmCntrMeasureHeaderTEO.ATTRIBUTE11,
       BmCntrMeasureHeaderTEO.ATTRIBUTE12,
       BmCntrMeasureHeaderTEO.ATTRIBUTE13,
       BmCntrMeasureHeaderTEO.ATTRIBUTE14,
       BmCntrMeasureHeaderTEO.ATTRIBUTE15,
       BmCntrMeasureHeaderTEO.Comments,
       BmCntrMeasureHeaderTEO.Graphic_Progress,
       BmCntrMeasureHeaderTEO.Measure_Date,
       ve.cntr_verify_header_id,
       ve.document_number                           as CVH_DOCNO,
       ve.verify_start_date,
       ve.verify_end_date,
       ve.ower_confirm_quantity,
       ve.other_subcntr_output,
       ve.cntr_visa_confirm_quantity,
       ve.Ve_cntr_measure_header_id,
BmCntrMeasureHeaderTEO.org_id,
BmCntrMeasureHeaderTEO.PRO_SUBCNTR_FLAG,
       BmCntrMeasureHeaderTEO.VAT_AMOUNT,
       (select  NVL(t.tax_rate,NULL)
          from bm.bm_cntr_list_header_t t
         where T.CNTR_ID =  BmCntrMeasureHeaderTEO.CNTR_ID
         AND ROWNUM = 1
         ) as  tax_rate,
       BmCntrMeasureHeaderTEO.CUMULATIVE_PRODUCTION_AMOUNT
 FROM BM.BM_CNTR_MEASURE_HEADER_T BmCntrMeasureHeaderTEO,
        ( select      cvh.cntr_verify_header_id,
       cvh.document_number,
       cvh.verify_start_date,
       cvh.verify_end_date,
       cvh.ower_confirm_quantity,
       cvh.other_subcntr_output,
       cvh.cntr_visa_confirm_quantity,
       vl.cntr_measure_header_id as Ve_cntr_measure_header_id
          From bm.bm_cntr_verify_header_t cvh, bm.bm_cntr_verify_line_t vl
         where cvh.cntr_verify_header_id = vl.cntr_verify_header_id
           and cvh.status <> 'EDIT' ) ve,
            bm.bm_cntr_list_header_t    BmCntrListHeaderTEO,
       stm.stm_contract_headers_v  sch,
       stm.stm_projects_v          supa,
       stm.stm_customers_v         scv,
       stm.stm_employees_v sev
where BmCntrMeasureHeaderTEO.cntr_measure_header_id = ve.Ve_cntr_measure_header_id(+)

 and BmCntrListHeaderTEO.Cntr_List_Header_Id(+) =
       BmCntrMeasureHeaderTEO.Cntr_List_Number
   and BmCntrMeasureHeaderTEO.Cntr_Id = sch.K_HEADER_ID(+)
   and sch.K_TYPE_CODE(+) in ('A', 'B', 'C', 'H', 'G')
   and supa.project_id(+) = BmCntrMeasureHeaderTEO.Project_Id
   and scv.CUSTOMER_ID(+) = BmCntrMeasureHeaderTEO.Customer_Id
   and BmCntrMeasureHeaderTEO.Prepared_By = sev.employee_id(+)]]>
  </SQLQuery>
